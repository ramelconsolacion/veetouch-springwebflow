<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/webflow
        http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd"
	parent="parent-flow">
	
	<var name="galleriaBean" class="org.veetouch.primefaces.product.GalleriaOverall"/>
	
	<on-start>
		<evaluate expression="menuNavigator.setPage('PRODUCT')"></evaluate>
	</on-start>
	
	<view-state id="product">
		
	</view-state>
	
	<view-state id="overall">
		<on-entry>
			<evaluate expression="vt_productService.listAllProduct()" result="viewScope.products" result-type="dataModel"></evaluate>
		</on-entry>
	</view-state>
	


</flow>